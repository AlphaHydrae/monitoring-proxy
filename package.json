  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/sqrs/monitoring-proxy.git"
  },
  "keywords": [
    "prometheus",
    "node-exporter",
    "monitoring",
    "proxy",
    "ssh"
  ],
  "author": "Simon Oulevay <npm@alphahydrae.ch>",
  "license": "MIT",
  "scripts": {
    "doctoc": "doctoc --github --notitle README.md && prettier --write README.md",
